[global_config]
  title_inactive_bg_color = "#000000"
  title_inactive_fg_color = "#00cc00"
  title_transmit_bg_color = "#000000"
  title_transmit_fg_color = "#00cc00"
[keybindings]
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_color = "#111111"
    background_darkness = 0.85
    cursor_color = "#00cc00"
    font = Ubuntu Mono derivative Powerline 12
    foreground_color = "#00cc00"
    palette = "#000000:#55ff55:#00cc00:#00cc00:#005500:#55ff55:#00cc00:#00cc00:#000000:#55ff55:#55ff55:#55ff55:#005500:#55ff55:#55ff55:#00cc00"
    use_system_font = False
